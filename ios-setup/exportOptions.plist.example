<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- 
    Export Options for ShineCycle iOS IPA
    
    Update the following before building for production:
    1. teamID: Your Apple Developer Team ID (found in Apple Developer Portal)
    2. method: Choose distribution method
       - app-store: For App Store submission
       - ad-hoc: For limited device testing
       - enterprise: For enterprise distribution
       - development: For development testing
    -->
    
    <!-- Distribution Method -->
    <key>method</key>
    <string>app-store</string>
    
    <!-- Your Apple Developer Team ID (REQUIRED) -->
    <!-- Find this in: https://developer.apple.com/account -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID_HERE</string>
    
    <!-- Provisioning Profile -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.istri.laundry</key>
        <string>YOUR_PROVISIONING_PROFILE_NAME</string>
    </dict>
    
    <!-- Bitcode (deprecated in Xcode 14+) -->
    <key>uploadBitcode</key>
    <false/>
    
    <!-- Upload Symbols for crash reporting -->
    <key>uploadSymbols</key>
    <true/>
    
    <!-- Compile Bitcode -->
    <key>compileBitcode</key>
    <false/>
    
    <!-- Thinning (for App Store) -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>
    
    <!-- Strip Swift Symbols -->
    <key>stripSwiftSymbols</key>
    <true/>
    
    <!-- Signing Certificate -->
    <key>signingCertificate</key>
    <string>Apple Distribution</string>
    
    <!-- Signing Style -->
    <key>signingStyle</key>
    <string>manual</string>
    
    <!-- Destination (for App Store Connect) -->
    <key>destination</key>
    <string>upload</string>
</dict>
</plist>
